#
# Exclude CentOS files
- .autorelabel
- .readahead
- /dev/*
- /etc/.updated
- /etc/machine-id
- /etc/aliases.db
- /etc/audit/audit.rules
- /etc/audit/audit.rules.prev
- /etc/lvm/cache/*
- /etc/mtab
- /etc/udev/hwdb.bin
- /media/*
- /proc/*
- /root/.bash_history
- /run/*
- /sys/*
- /tmp/*
- /var/.updated
- /var/cache/yum/*
- /var/lib/dhclient/*
- /var/lib/dhcpcd/*
- /var/lib/gssproxy/default.sock
- /var/lib/ldap/*
- /var/lib/logrotate.status
- /var/lib/misc/postfix.aliasesdb-stamp
- /var/lib/mlocate/*
- /var/lib/NetworkManager/NetworkManager.state
- /var/lib/NetworkManager/timestamps
- /var/lib/nfs/*
- /var/lib/ntp/drift
- /var/lib/ntp/proc/*
- /var/lib/postfix/master.lock
- /var/lib/rpm/__db.*
- /var/lib/sss/*
- /var/lib/systemd/random-seed
- /var/lib/systemd/catalog/database
- /var/lib/rsyslog/imjournal.state
- /var/lock/subsys/*
- /var/log/*
- /var/run/*.pid
- /var/run/*/*.pid
- /var/spool/anacron/*
- /var/spool/mail/*
- /var/spool/postfix/*
- /var/tmp/*
#
# Excludes files generated by luna
- /etc/fstab
- /etc/hostname
- /etc/HOSTNAME
- /etc/sysconfig/network
- /etc/sysconfig/network-scripts/ifcfg-*
